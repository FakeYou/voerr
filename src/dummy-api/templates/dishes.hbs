{
	"dishes": [
		{{#repeat 6}} 
		{
			"id": {{index}},
			"name": "{{lorem_sentence}}",
			"description": "{{text}}",
			"cook": {
				"id": {{uniqueIndex}},
				"name": "{{name_findName}}",
				"email": "{{internet_email}}",
				"description": "{{text}}",
				"location": "{{address_streetName}} {{random_number max=200}}, {{address_city}}",
				"coordinates": {
					"lat": {{address_latitude}},
					"long": {{address_longitude}}
				}
			},
			"allergies": [
				{{#repeat 0 5}}
				{
					"id": {{index}},
					"name": "{{allergy_name}}",
					"description": "{{allergy_description}}"
				}
				{{/repeat}}
			],
			"categories": [
				{ "category": "Menugang", "value": "{{category_menucourse}}" },
				{ "category": "Soort gerecht", "value": "{{category_typedish}}" },
				{ "category": "Recepten met", "value": "{{category_recipewith}}" },
				{ "category": "Speciale wensen", "value": "{{category_specialdemands}}" },
				{ "category": "Seizoen", "value": "{{category_season}}" },
				{ "category": "Keuken", "value": "{{category_kitchen}}" }
			]
		}
		{{/repeat}}
